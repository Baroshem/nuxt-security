{
  "name": "nuxt-security-no-cors",
  "version": "0.13.0",
  "license": "MIT",
  "type": "module",
  "homepage": "https://nuxt-security.vercel.app",
  "description": "ðŸ›¡ Security Module for Nuxt based on OWASP Top 10 and Helmet",
  "repository": {
    "url": "https://github.com/insomnia-creator/nuxt-security-NO-CORS"
  },
  "keywords": [
    "nuxt",
    "vue",
    "security",
    "owasp",
    "helmet",
    "basic-auth",
    "rate-limit",
    "xss",
    "cors",
    "request-size-limit",
    "allowed-http-methods",
    "csrf"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxt-module-build --stub && nuxi prepare playground && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:start": "nuxi start playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.ts,.vue",
    "test": "vitest run --silent",
    "test:watch": "vitest watch",
    "docs": "cd docs && yarn dev",
    "preview": "nuxi preview playground",
    "stackblitz": "cd .stackblitz && yarn && yarn dev"
  },
  "dependencies": {
    "@nuxt/kit": "^3.2.2",
    "basic-auth": "^2.0.1",
    "defu": "^6.1.1",
    "limiter": "^2.1.0",
    "memory-cache": "^0.2.0",
    "nuxt-csurf": "^1.1.0",
    "pathe": "^1.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^3.2.2",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxt/test-utils": "^3.2.2",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.14.4",
    "eslint": "latest",
    "nuxt": "^3.2.2",
    "vitest": "^0.28.5"
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "yarn stackblitz"
  }
}
